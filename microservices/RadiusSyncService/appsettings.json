{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Debug"
    }
  },
  "AllowedHosts": "*",
  "SignalR": {
    "HubUrl": "http://localhost:5000/hubs/microservices",
    "ReconnectDelaySeconds": 5,
    "HeartbeatIntervalSeconds": 30
  }
}
